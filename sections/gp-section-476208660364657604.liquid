{%- liquid
      assign productHandles = 'the-hidden-snowboard,the-minimal-snowboard,the-collection-snowboard-liquid,the-collection-snowboard-hydrogen'| split: ','
      assign products = null
      assign target_collection = null
      assign productSrc = 'PickProduct'
      assign c = 0

      if productSrc == 'DynamicCollection'
        assign target_collection = collection 
      else
        if latest != 'latest'
          for collection in collections
            if collection.id == latest 
              assign target_collection = collection 
              break 
            endif
          endfor 
        endif
      endif

      if productSrc == 'PickProduct'
        if 4 == 0
          paginate collections.all.products by 100000
            assign products = collections.all.products  | sort : "created_at"  | reverse 
          endpaginate
        endif
      else
        if 'latest' == 'latest'
          assign latest_collection = collections | sort: 'updated_at' | first 
          paginate latest_collection.products by 4
            assign products = latest_collection.products
          endpaginate
        else
          paginate target_collection.products by 4
            assign products = target_collection.products
          endpaginate
        endif
      endif
      -%}
    
    
      
